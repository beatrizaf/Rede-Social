create schema redesocial
create table redesocial.user(
  nome varchar(100) primary key,
  rua varchar(200),
  numero integer,
  bairro varchar(100),
  cidade varchar(100),
  data_nsc date,
  telefone varchar(14));
create table redesocial.genesis(
  email varchar(200)primary key,
  senha varchar(18));
create table redesocial.perfil(
  bibliografia varchar(1000),
  nomeuser varchar(100),
  foreign key (nomeuser)
  references redesocial.user(nome));
